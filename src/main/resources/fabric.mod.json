{
  "schemaVersion": 1,
  "id": "rpbypass",
  "version": "${version}",

  "name": "Resource Pack Bypass",
  "description": "Lets you bypass 1.19.3 force server resource packs.",
  "authors": [
    "Coderx_Gamer"
  ],
  "contact": {
    "homepage": "https://github.com/Coderx-Gamer/rp-bypass/releases",
    "sources": "https://github.com/Coderx-Gamer/rp-bypass/"
  },

  "license": "CC0-1.0",
  "icon": "assets/rpbypass/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "net.rpbypass.MainClient"
    ]
  },
  "mixins": [
    "rpbypass.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.14",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
